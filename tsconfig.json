{
	"compilerOptions": {
		"declaration": true,
		"removeComments": true,
		"target": "esnext",
		"moduleResolution": "node",
		"newLine": "lf",
		"module": "commonjs",
		"downlevelIteration": true,
		"sourceMap": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"esModuleInterop": true,
		"strictNullChecks": true
	}
}
