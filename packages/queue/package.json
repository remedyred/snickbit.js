{
	"name": "@snickbit/queue",
	"version": "1.2.15",
	"description": "Fluid queueing library for any function or promise",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"build:types": "tsc --emitDeclarationOnly --declarationDir dist",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"dev:types": "tsc --emitDeclarationOnly --declarationMap --declarationDir .types",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"index": "indexer",
		"release": "semantic-release",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"semantic-release-monorepo",
			"@snickbit/semantic-release"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@snickbit/utilities": "^3.4.0",
		"lite-fifo": "^0.3.0"
	},
	"devDependencies": {
		"@snickbit/indexer": "2.5.17",
		"@types/jest": "28.1.8",
		"jest": "28.1.3",
		"shx": "0.3.4",
		"ts-jest": "28.0.8",
		"tsup": "6.2.3",
		"typedoc": "0.23.12",
		"typedoc-plugin-markdown": "3.13.6",
		"typescript": "4.8.3"
	},
	"engines": {
		"node": ">= 12"
	}
}
