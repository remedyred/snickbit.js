{
	"name": "@snickbit/queue",
	"version": "1.2.2",
	"description": "Fluid queueing library for any function or promise",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist ",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"docs": "typedoc",
		"index": "indexer",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run clean && pnpm run build --minify",
		"test": "jest",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@snickbit/utilities": "^3.2.1",
		"lite-fifo": "^0.3.0"
	},
	"devDependencies": {
		"@snickbit/indexer": "2.5.7",
		"@types/jest": "28.1.6",
		"jest": "28.1.3",
		"shx": "0.3.4",
		"ts-jest": "28.0.7",
		"tsup": "6.2.1",
		"typedoc": "0.23.10",
		"typedoc-plugin-markdown": "3.13.4",
		"typedoc-plugin-no-inherit": "1.3.1",
		"typescript": "4.7.4"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
